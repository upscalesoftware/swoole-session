{
    "name": "upscale/swoole-session",
    "description": "PHP sessions compatibility with Swoole web-server",
    "version": "2.0.0",
    "keywords": ["session", "Swoole", "server", "PHP", "compatibility"],
    "homepage": "https://github.com/upscalesoftware/swoole-session",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Upscale Software Team",
            "email": "info@upscalesoftware.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-swoole": "^4.0||^5.0"
    },
    "require-dev": {
        "ext-curl": "*",
        "phpunit/phpunit": "^9.5",
        "upscale/swoole-launchpad": "^2.0"
    },
    "autoload": {
        "psr-4": {"Upscale\\Swoole\\Session\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {
            "Upscale\\Swoole\\Session\\Tests\\": "tests/",
            "Upscale\\Swoole\\Launchpad\\Tests\\": "vendor/upscale/swoole-launchpad/tests/"
        }
    }
}
